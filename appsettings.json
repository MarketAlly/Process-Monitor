{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ProcessMonitor": {
    "MonitoringIntervalSeconds": 10,
    "EnableDebugLogging": false,
    "LogRetentionDays": 30,
    "AllowedPaths": [
      "C:\\Windows\\System32",
      "C:\\Program Files",
      "C:\\Program Files (x86)",
      "C:\\test.exe"
    ],
    "EnablePathValidation": true,
    "EnablePerformanceMonitoring": true,
    "MaxConcurrentStarts": 5
  }
}